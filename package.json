{
  "name": "movie-club",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run migrate && npm run type-check && npm run lint && npm test && vite build",
    "serve": "vite preview",
    "lint": "eslint src migrations netlify/functions lib scripts --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit --composite false",
    "migrate": "tsx ./migrations/schemaMigrator.ts && npm run codegen",
    "migrate:dev": "tsx --env-file=.env ./migrations/schemaMigrator.ts && npm run codegen",
    "migrate:down": "tsx --env-file=.env ./migrations/schemaMigrator.ts down && npm run codegen",
    "migrate:data": "tsx --env-file=.env ./migrations/dataMigrator.ts",
    "codegen": "kysely-codegen --dialect postgres --runtime-enums --out-file lib/types/generated/db.ts --exclude-pattern crdb_internal.*",
    "db:snapshot": "tsx --env-file=.env scripts/db-snapshot.ts",
    "db:spawn": "tsx --env-file=.env scripts/db-spawn.ts",
    "db:list": "tsx --env-file=.env scripts/db-list.ts",
    "db:cleanup": "tsx --env-file=.env scripts/db-cleanup.ts"
  },
  "dependencies": {
    "@cubos/kysely-cockroach": "^1.0.0",
    "@headlessui/vue": "^1.7.23",
    "@mdi/js": "^7.4.47",
    "@netlify/functions": "^5.1.2",
    "@tanstack/query-persist-client-core": "^4.29.7",
    "@tanstack/query-sync-storage-persister": "^4.41.0",
    "@tanstack/vue-query": "^4.41.0",
    "@tanstack/vue-table": "^8.21.3",
    "ag-charts-vue3": "^11.3.2",
    "animate.css": "^4.1.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.4.7",
    "cloudinary": "^2.8.0",
    "kysely": "^0.27.6",
    "lambda-multipart-parser": "^1.0.1",
    "luxon": "^3.7.2",
    "mdi-vue": "^3.0.13",
    "path-parser": "^6.1.0",
    "pg": "^8.16.3",
    "pinia": "^2.3.1",
    "randomcolor": "^0.6.2",
    "resend": "^6.6.0",
    "vue": "^3.3.4",
    "vue-draggable-next": "^2.3.0",
    "vue-router": "^4.6.4",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@pinia/testing": "^0.0.16",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^7.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "@types/randomcolor": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.15.0",
    "jsdom": "^21.1.2",
    "kysely-codegen": "^0.15.0",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.15",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^3.1.8"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
